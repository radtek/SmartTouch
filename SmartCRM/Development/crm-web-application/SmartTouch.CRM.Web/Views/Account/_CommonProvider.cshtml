                <label class="control-label">[|Host|] <span class="required">*</span></label>
                <!--/ko-->
                <!--ko if: (MailProviderID == 2 || MailProviderID == 3)-->
                <input type="text" class="form-control" placeholder="[|Host|]" data-bind="value:Host,valueUpdate: 'afterkeydown'" />
