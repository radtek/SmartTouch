@using SmartTouch.CRM.Entities;

<script type="text/html" id="providertype-transactional">
    <div id="transactionalprovidermail1" data-bind="if:MailProviderType==@((byte)MailType.TransactionalEmail) && MailProviderID >
        0 ,visible:MailProviderID == parseInt($parent.EmailProviderType())">
        <div class="clearfix formsubheading">
            <span data-bind="visible:MailProviderID==2" id="SendGridID">
                [|SendGrid Configuration|]
            </span>
            <span data-bind="visible:MailProviderID==3" id="MailChimpID">
                [|Mailchimp Configuration|]
            </span>
            <span data-bind="visible:MailProviderID==4" id="VmtaID">
                [|VMTA Configuration|]
            </span>
            <span class="mlm">
                <input type="radio" name="emailprovider" data-bind="checked: IsDefault+'',value:'true',click:$parent.DefaultProvider.bind($data)" />
