            </div>
        </div>
        <div class="clearfix">
            <div class="form-group">
                <!--ko ifnot: (MailProviderID == 4 && $parent.IsAccountStAdmin !== 'True')-->
                <label class="control-label">
                    User Name
                    <span data-bind="visible:MailProviderID==4||MailProviderID==2" class=" required">*</span>
                </label>
                <!--/ko-->
                <!--ko if: (MailProviderID == 2)-->
