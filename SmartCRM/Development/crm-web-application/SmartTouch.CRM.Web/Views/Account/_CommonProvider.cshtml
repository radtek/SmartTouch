                <!--/ko-->
                @*<!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'False')-->
                    <input type="text" class="form-control" placeholder="[|User Name|]" readonly="readonly" data-bind="value:UserName,valueUpdate: 'afterkeydown'" />
                    <!--/ko-->*@
            </div>
            <div class="form-group">
                <!--ko ifnot: (MailProviderID == 4 && $parent.IsAccountStAdmin !== 'True')-->
                <label class="control-label">[|Password|] <span class="required" data-bind="visible:MailProviderID==4||MailProviderID==2">*</span></label>
                <!--/ko-->
                <!--ko if: (MailProviderID == 2)-->
                <input type="password" class="form-control" placeholder="[|Password|]" data-bind="value:Password,valueUpdate: 'afterkeydown'" />
                <!--/ko-->
                <!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')-->
                <input type="password" class="form-control" placeholder="[|Password|]" data-bind="value:Password,valueUpdate: 'afterkeydown'" />
                <!--/ko-->
                @*<!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'False')-->
                    <input type="password" class="form-control" placeholder="[|Password|]" readonly="readonly" data-bind="value:Password,valueUpdate: 'afterkeydown'" />
                    <!--/ko-->*@
            </div>

        </div>
        <!--ko ifnot: (MailProviderID == 4 && $parent.IsAccountStAdmin !== 'True')-->
        <div class="clearfix">
            <div class="form-group">
                <label class="control-label">[|Sender Email|] <span class="required" data-bind="visible:MailProviderID!==2">*</span></label>
                <input type="email" class="form-control" placeholder="[|Sender Email|]" data-bind="value:Email,valueUpdate: 'afterkeydown'" />
            </div>
            <div class="form-group">
                <label class="control-label">[|Sender Friendly Name|]</label>
                <input type="text" class="form-control" placeholder="[|Sender Friendly Name|]" data-bind="value:SenderFriendlyName" />
            </div>

        </div>
        <!--/ko-->

        <div class="clearfix" id="VMTAConfiguration" data-bind="visible:MailProviderID==4">
            <div data-bind="if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')">
                <!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')-->
                <div class="form-group" id="SenderDomain" data-bind="visible:MailProviderID==4">
                    <!--ko if: (MailProviderID == 4) -->
                    <label class="control-label">[|Sender's Domain|] </label>
                    <!--/ko-->
                    <!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')-->
                    <input type="text" class="form-control" placeholder="[|Sender Domain|]" data-bind="value:SenderDomain" />
                    <!--/ko-->
                    @*<!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'False')-->
            <input type="text" class="form-control" placeholder="[|Sender Domain|]" readonly="readonly" data-bind="value:SenderDomain" />
            <!--/ko-->*@
                </div>
                <!--/ko-->

                <div class="form-group" id="ImageDomain" data-bind="visible:MailProviderID==4">
                    <!--ko if: (MailProviderID == 4) -->
                    <label class="control-label">Image Domain <span class="required">*</span></label>
                    <input data-bind="kendoDropDownList: {dataTextField:'Domain',dataValueField:'ImageDomainId', data: $root.ImageDomains, value: ImageDomainId,optionLabel:'Select...'}" />
                    <span class="validationMessage" data-bind="validationMessage:ImageDomainId"></span>
                    <!--/ko-->

                </div>
            </div>
            <div data-bind="if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'False')">
                <div class="form-group">
                    <div class="alert alert-warning">
                        <i class="icon st-icon-spam"></i>
                        [|Contact SmartTouch administrator to change.|]
                    </div>
                </div> 
            </div>
        </div>
        <div class="clearfix">
            <div data-bind="if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')">
                <div class="form-group" id="VMTA" data-bind="visible:MailProviderID==4">
                    <!--ko if: (MailProviderID == 4) -->
                    <label class="control-label">[|SmartTouch|]<span class="required">*</span></label>
                    <!--/ko-->
                    <!--ko if: (MailProviderID == 4 && $parent.IsAccountStAdmin == 'True')-->
                    <input type="text" class="form-control" placeholder="[|SmartTouch|]" data-bind="value:VMTA" />
                    <!--/ko-->
