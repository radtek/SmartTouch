<script type="text/html" id="control-template">
    <div data-bind="visible:!IsStyleTag(),attr:{id:'li-'+Id(), name:'campaign-control'}">
        <div class="st-layout">
            <span data-bind="value:Id($index())"></span>
            <div class="st-droparea-controls" data-bind="if:!$parent.editMode()">
                <a href="javascript:void(0)" class="sort"><i class="icon st-icon-move"></i></a>
                <a href="javascript:void(0)" data-bind="click: $parent.CopyControl.bind($data,$index())" class="copy"><i class="icon st-icon-windows"></i></a>
                <a href="javascript:void(0)" class="delete" data-bind="click: $parent.DeleteControl"><i class="icon st-icon-bin-3"></i></a>
            </div>
            <!-- ko if: TemplateSourceId() == 'fromLayout' -->
            <div style='display: block;'
                 data-bind="attr:{id:'control-content-' + Id()}, html:EditedHTML,value:EditedHTML()">
            </div>
            <!-- /ko -->
            <!-- ko ifnot: TemplateSourceId() == 'fromLayout' -->
            <div style='display: block;'
                 data-bind="template: { name: TemplateSourceId },attr:{id:'control-content-' + Id()},value:EditedHTML">
            </div>
            <!-- /ko -->
        </div>
    </div>
</script>

